  "private": true,
  "devDependencies": {
    "@types/node": "^14.14.19",
    "@typescript-eslint/eslint-plugin": "^4.11.0",
    "@typescript-eslint/parser": "^4.11.0",
    "@wayward/types": "^2.9.6-beta",
    "asar": "^3.0.3",
